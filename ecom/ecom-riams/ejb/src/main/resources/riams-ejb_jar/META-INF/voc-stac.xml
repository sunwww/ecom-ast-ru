		    	queryAppend="(vmhc.dateTo is null or vmhc.dateTo >=current_date)"
		    	order=""
		    	queried="( vmhc.name like upper(:query) or  vmhc.code like upper(:querId))"
		    	names = "vmhc.code||' '||vmhc.name"
		    	
		/>
	</voc>

