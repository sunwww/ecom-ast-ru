    <voc id='vocOncologyN001' type='EntityVocService'>
        <EntityVocService
                entity  = 'ru.ecom.oncological.ejb.domain.voc.VocOncologyN001'
                names   = 'name'
                queried = 'code,name'
                queryAppend="(finishdate is null or finishdate>=current_date)"
        />
    </voc>
    <voc id="vocOncologyN001Code" type="NativeVocService">
        <NativeVocService
                from="VocOncologyN001 voc"
                nameId="voc.code"
                order="voc.name"
                names="voc.code||' '||voc.name"
        />
    </voc>

    <voc id='vocOncologyN002' type='NativeVocService'>
        <NativeVocService
                from = "VocOncologyN002 voc"
                nameId= "voc.id"
                order = "voc.ds"
                names = "voc.name"
                queryAppend="(finishdate is null or finishdate>=current_date)"
        />
    </voc>

    <voc id='vocOncologyN002parent' type='NativeVocService'>
        <NativeVocService
                from = "voconcologyn002 n002"
                parent='case when (select count(*) from voconcologyN002 where ds=:parent and (finishdate is null or finishdate>=current_date))>0 then ds=:parent
                else case when (select count(*) from voconcologyN002 where ds=substring(:parent from 1 for 3) and (finishdate is null or finishdate>=current_date))>0 then  ds=substring(:parent from 1 for 3)
                else ds is null  end end'
                nameId= "n002.id"
                names = "n002.name"
                queryAppend="(finishdate is null or finishdate>=current_date)"
        />
    </voc>

    <voc id='vocOncologyN003parent' type='NativeVocService'>
        <NativeVocService
                from = "voconcologyn003 n003"
                parent='case when (select count(*) from voconcologyn003 where ds=:parent and (finishdate is null or finishdate>=current_date))>0 then ds=:parent
                else case when (select count(*) from voconcologyn003 where ds=substring(:parent from 1 for 3) and (finishdate is null or finishdate>=current_date))>0 then  ds=substring(:parent from 1 for 3)
                else ds is null  end end'
                nameId= "n003.id"
                names = "case when n003.name is not null and n003.name!='' then name else n003.tumorcode end"
                queryAppend="(finishdate is null or finishdate>=current_date)"
        />
    </voc>

